<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>Online_Ceramics_Store</RootNamespace>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(RunConfiguration)' == 'http' " />
  <ItemGroup>
    <PackageReference Include="MySqlConnector" Version="2.3.5" />
    <PackageReference Include="MySqlConnector.DependencyInjection" Version="2.3.5" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Views\ShoppingCart\" />
    <None Remove="Views\Products\" />
    <None Remove="Views\Account\" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Views\ShoppingCart\" />
    <Folder Include="Views\Products\" />
    <Folder Include="wwwroot\Images\" />
    <Folder Include="Views\Account\" />
  </ItemGroup>
  <ItemGroup>
    <None Include="wwwroot\Images\.DS_Store" />
    <None Include="wwwroot\Images\KalVahomer-1.jpg" />
    <None Include="wwwroot\Images\KalVahomer-2.jpg" />
    <None Include="wwwroot\Images\KalVahomer-3.jpg" />
    <None Include="wwwroot\Images\KalVahomer-4.jpg" />
    <None Include="wwwroot\Images\KalVahomer-5.jpg">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="wwwroot\Images\KalVahomer-6.jpg" />
  </ItemGroup>
  <ItemGroup>
    <None Update="Images\KalVahomer-5.jpg">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
  <ItemGroup>
    <Content Remove="wwwroot\Images\.DS_Store" />
    <Content Remove="wwwroot\Images\KalVahomer-1.jpg" />
    <Content Remove="wwwroot\Images\KalVahomer-2.jpg" />
    <Content Remove="wwwroot\Images\KalVahomer-3.jpg" />
    <Content Remove="wwwroot\Images\KalVahomer-4.jpg" />
    <Content Remove="wwwroot\Images\KalVahomer-5.jpg" />
    <Content Remove="wwwroot\Images\KalVahomer-6.jpg" />
    <Content Remove="wwwroot\Images\" />
  </ItemGroup>
</Project>
