@model List<Online_Ceramics_Store.Models.Product>

<h2>Products</h2>

<ul>
    @foreach (var product in Model)
    {
        <li>@product.Name</li>
        <li>@product.ProId</li>
        <li>@product.Dep</li>
        <li>@product.Brand</li>
    }
</ul>
